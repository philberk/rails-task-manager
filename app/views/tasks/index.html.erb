<div class= "header">
<h1>Task List</h1>
</div>

<ul>
  <% @tasks.each do |task| %>
    <li>  <%= link_to task.title, task_path(task) %> <input type="checkbox" name="completed"> </li>
  <%= link_to "Delete", task_path(task),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
  <% end %>
</ul>
